server {
    listen 80;
    listen [::]:80;

    root /var/www/ai-ticulate-client;
    index index.html;

    server_name localhost:8080;

    location / {
        try_files $uri $uri/ /index.html =404;
    }

    location /api/ {
        proxy_pass http://backend:8080/;
        include proxy_pass;
    }
}
